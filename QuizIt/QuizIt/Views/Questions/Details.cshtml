@model QuizIt.Models.Question

@{
    ViewData["Title"] = "Details";
}

<h2>Detaljer</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Fråga
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TrackQuestion)
        </dd>
        <dt>
            Svar
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Answer)
        </dd>
        <dt>
            Låt
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TrackTitle)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Ändra</a> |
    <a asp-action="Index">Se alla frågor</a>
</div>
